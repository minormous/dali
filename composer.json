{
    "name": "minormous/dali",
    "description": "The Database Abstraction LIbrary for Minormous Framework",
    "type": "library",
    "require": {
        "doctrine/dbal": "^3.1",
        "minormous/metabolize": "dev-main",
        "psr/log": "^1.1",
        "webmozart/assert": "^1.10",
        "davedevelopment/phpmig": "^1.7",
        "rdlowrey/auryn": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.9",
        "symfony/var-dumper": "^5.3"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Minormous\\Dali\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Dali\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Marisa",
            "email": "marisa@clardy.eu"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:minormous/metabolize.git"
        }
    ]
}
